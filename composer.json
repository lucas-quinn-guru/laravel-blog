{
    "name": "lucas-quinn-guru/laravel-blog",
    "description": "Front end blog and posts with out of the box with blog and post management backend.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Lucas Quinn",
            "email": "lucas.quinn@lucasquinn.guru"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1.3"
    },
    "autoload": {
        "psr-4": {
            "LucasQuinnGuru\\LaravelBlog\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LucasQuinnGuru\\LaravelBlog\\LaravelBlogServiceProvider"
            ]
        }
    }
}
